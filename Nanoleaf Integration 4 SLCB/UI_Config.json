{
    "output_file": "NI4SLCB.json",
    "chat_command": {
        "type": "textbox",
        "label": "Command (usage: {command} {effectName} {brightness})",
        "value": "!nl",
        "tooltip": "Master chat command",
        "group": "Master chat command"
    },
    "chat_brightness": {
        "type": "checkbox",
        "label": "Allow brightness parameter",
        "value": true,
        "tooltip": "Allow brightness parameter",
        "group": "Master chat command"
    },
    "chat_command_permission": {
        "type": "dropdown",
        "label": "Permission",
        "value": "Moderator",
        "tooltip": "Permission",
        "items": [
            "Everyone",
            "Regular",
            "Subscriber",
            "Moderator",
            "Editor",
            "Caster"
        ],
        "group": "Master chat command"
    },
    "chat_command_onEvent": {
        "type": "textbox",
